{
    "name": "employeedashboard",
    "version": "1.0.0",
    "description": "employeed dashboard application",
    "main": "index.js",
    "scripts": {
        "webpack": "webpack",
        "format": "prettier  \"src/**/*.(js|css|jsx)\" --write",
        "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
        "webpack-dev-server": "webpack-dev-server",
        "prod:build": "npm run webpack -- --env.mode production",
		"prod:compression": "npm run prod:build -- --env.presets compression",
		"prod:analyze": "npm run prod:build -- --env.presets analyze",
		"prod:preproduction": "npm run prod:build -- --env.presets compression --env.presets analyze"
        "prod:watch": "npm run prod:build -- --watch",
        "dev:build": "npm run webpack -- --env.mode development",
        "dev:watch": "npm run dev:build -- --watch",
        "dev:server": "npm run webpack-dev-server -- --env.mode development",
        "dev:hot": "npm run dev:server --hot"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^4.2.2",
        "eslint": "^7.8.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.6",
        "file-loader": "^6.1.0",
        "html-webpack-plugin": "^4.4.1",
        "mini-css-extract-plugin": "^0.11.0",
        "prettier": "^2.1.1",
        "style-loader": "^1.2.1",
        "url-loader": "^4.1.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.1.3"
    },
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
		"compression-webpack-plugin": "^2.0.0",
    }
}
